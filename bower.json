{
  "name": "storystore",
  "version": "1",
  "homepage": "https://github.com/tanyaburlakova/storystore",
  "authors": [
    "fatuk <ta.burlakova@gmail.com>"
  ],
  "moduleType": [
    "amd"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "normalize.less": {
      "ignore": "normalize.less"
    },
    "slick-carousel": {
      "main": "slick/slick.js"
    },
    "font-awesome-less": {
      "main": [
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.svg",
        "less/font-awesome.less"
      ]
    }
  },
  "dependencies": {
    "jquery": "~3.2.1",
    "normalize.less": "^1.0.0",
    "slick-carousel": "^1.6.0",
    "components-font-awesome": "^4.7.0"
  }
}
